{
  "name": "react-init",
  "version": "0.0.0",
  "description": "first play with react and isomorphism",
  "main": "index.js",
  "browserify": {
    "transform": [
      [
        "reactify",
        {
          "es6": false
        }
      ]
    ]
  },
  "dependencies": {
    "async": "^1.3.0",
    "body-parser": "^1.12.2",
    "browserify": "^9.0.3",
    "chart.js": "^1.0.2",
    "eventemitter": "^0.3.3",
    "express": "^4.12.3",
    "flux": "^2.0.3",
    "jsdom": "^3.1.2",
    "jsx": "^0.9.89",
    "morgan": "^1.5.2",
    "node-jsx": "^0.12.4",
    "nodemon": "^1.3.7",
    "object-assign": "^3.0.0",
    "parallelshell": "^1.1.1",
    "react": "^0.13.1",
    "react-chartjs-commonjs": "0.0.3",
    "react-router": "^0.13.1",
    "reactify": "^1.1.0",
    "request": "^2.55.0",
    "serve-static": "^1.9.2",
    "watchify": "^2.6.0"
  },
  "devDependencies": {},
  "scripts": {
    "build-js": "mkdir -p public/js/ && browserify components/Routes.jsx -t reactify -o public/js/bundle.js",
    "build-sass": "mkdir -p public/css/ && sass sass/style.scss > public/css/style.css",
    "build": "npm run build-js && npm run build-sass",
    "watch-js": "watchify components/Routes.jsx -t reactify -o public/js/bundle.js",
    "watch-sass": "sass --watch sass:public/css",
    "watch": "npm run build && parallelshell 'npm run watch-js' 'npm run watch-sass'",
    "start": "nodemon index.js"
  },
  "author": "Jaz Lalli",
  "license": "MIT"
}
